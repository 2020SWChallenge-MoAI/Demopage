<% layout("layouts/default") %>
<%- partial("includes/left") %>
<div id="ToggleLeftAreaBtnContainer">
	<button class="btn btn-outline-secondary border-dark" type="button" id="ToggleLeftAreaBtn"><i class="fas fa-chevron-left"></i></button>
</div>
<div class="mh-100 h-100" id="RightArea">
	<div class="row">
		<div class="col cell" id="KeyWordConfigCell">
			<div class="cell-title">
				<p>키워드 추출</p>
			</div>
			<div class="cell-content">
				<div class="cell" id="KeywordModelVersionCell">
					<div class="cell-title">
						<p>모델 버전</p>
					</div>
					<div class="cell-content">
						<select class="form-control border-dark" id="KeywordModelVersion">
							<option value="v1.0.2">v1.0.2</option>
							<option value="current" selected>current</option>
						</select>
					</div>
				</div>
		
				<div class="cell" id="KeywordNumCell">
					<div class="cell-title">
						<p>개수</p>
					</div>
					<div class="cell-content">
						<div class="input-group">
							<div class="input-group-prepend">
								<button class="btn btn-outline-secondary border-dark btn-sm" type="button" id="KeywordNumDecBtn"><i class="fas fa-chevron-down"></i></button>
							</div>
							<input type="number" class="form-control border-dark input-number-no-control" id="KeywordNum" value="5" min="1">
							<div class="input-group-append">
								<button class="btn btn-outline-secondary border-dark btn-sm" type="button" id="KeywordNumIncBtn"><i class="fas fa-chevron-up"></i></button>
							</div>
						</div>
					</div>
				</div>

				<div class="cell" id="KeywordHistoryCell">
					<div class="cell-title">
						<p>질의 키워드</p>
					</div>
					<div class="cell-content">
						<input type="text" class="form-control border-dark" id="KeywordHistory">
					</div>
				</div>
			</div>
		</div>

		<div class="col cell" id="MainSentenceConfigCell">
			<div class="cell-title">
				<p>핵심 문장 추출</p>
			</div>
			<div class="cell-content">
				<div class="cell" id="MainSentenceModelVersionCell">
					<div class="cell-title">
						<p>모델 버전</p>
					</div>
					<div class="cell-content">
						<select class="form-control border-dark" id="MainSentenceModelVersion">
							<option value="v1.0.0">v1.0.0 - komoran tokenizer</option>
							<option value="v2.0.0">v2.0.0 - subword tokenizer</option>
							<option value="current" selected>v3.0.0 - Current - komoran + subword + duplication filter</option>
						</select>
					</div>
				</div>

				<div class="cell" id="MainSentenceNumCell">
					<div class="cell-title">
						<p>개수</p>
					</div>
					<div class="cell-content">
						<div class="input-group">
							<div class="input-group-prepend">
								<button class="btn btn-outline-secondary border-dark btn-sm" type="button" id="MainSentenceNumDecBtn"><i class="fas fa-chevron-down"></i></button>
							</div>
							<input type="number" class="form-control border-dark input-number-no-control" id="MainSentenceNum" value="5" min="1">
							<div class="input-group-append">
								<button class="btn btn-outline-secondary border-dark btn-sm" type="button" id="MainSentenceNumIncBtn"><i class="fas fa-chevron-up"></i></button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="cell d-flex justify-content-end">
		<button type="button" id="Submit-Btn" class="btn btn-primary d-flex align-items-center">
			<span class="btn-text">AI 분석</span>
			<span class="spinner-border spinner-border-sm d-none"></span>
			<span class="spinner-text d-none">&nbsp;분석 중...</span>
		</button>
	</div>

	<div class="cell border border-success rounded p-2 d-none" id="ResultCell-Success">
		<div class="cell-title">
			<p>분석 결과</p>
		</div>

		<div class="cell-content">
			<div class="row">
				<div class="cell col" id="KeywordCell">
					<div class="cell-title">
						<p>키워드</p>
					</div>
	
					<div class="cell-content">
					</div>
	
					<div class="d-flex justify-content-end">
						<button type="button" id="Keyword-Eval-Submit-Btn" class="btn btn-primary d-flex align-items-center">
							<span class="btn-text">키워드 평가 등록하기</span>
							<span class="spinner-border spinner-border-sm d-none"></span>
							<span class="spinner-text d-none">&nbsp;등록 중...</span>
						</button>
					</div>
				</div>

				<div class="cell col" id="NERCell">
